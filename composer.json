{
    "name": "admin/pro",
    "type": "project",
    "description": "专业版本",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "ext-zip": "*",
        "catchadmin/pro": "^0.7.1",
        "catchadmin/form": "^0.1.2",
        "laravel/framework": "^v12.7.2",
        "laravel/octane": "^v2.8.3",
        "laravel/tinker": "^v2.10.1",
        "overtrue/easy-sms": "^3.1.0",
        "yansongda/pay": "v3.7.10",
        "tymon/jwt-auth": "^2.2.0",
        "mews/captcha": "^3.4.5"
    },
    "require-dev": {
        "fakerphp/faker": "^v1.24.1",
        "laravel/pint": "^v1.21.2",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^v3.8.1",
        "itsgoingd/clockwork": "^v5.3.4",
        "knuckleswtf/scribe": "^5.1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "modules",
            "Pro\\Support\\": "support/",
            "\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd\" \"cd web && yarn dev\" \"php artisan serve\" --names='vite,server'"
        ],
        "pint": "./vendor/bin/pint ./pint.json"
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform-check": false,
        "secure-http": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.catchadmin.com",
            "only-dist": true,
            "options": {
                "ssl": {
                    "verify_peer": false,
                    "verify_peer_name": false
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}